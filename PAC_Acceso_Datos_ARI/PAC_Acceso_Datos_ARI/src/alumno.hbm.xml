<?xml version='1.0' encoding="UTF-8"?>
        <!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/HibernateMappingDTD5.3//EN"
                "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="hibernate.Alumno" table="Alumnos" catalog="MIBBDD">
    <id name="idAlumno">
        <column name="alumno_id"/>
        <generator class="assigned"></generator>
    </id>
    <property name="nombre" type="String"></property>
    <property name="nacionalidad" type="String"></property>
    <property name="edad" type="int"></property>
    <property name="sexo" type="String"></property>
    <property name="modulos" type="List"></property>
</class>
</hibernate-mapping>